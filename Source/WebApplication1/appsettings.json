{
  "Serilog": {
    "Using": [ "Serilog.Enrichers.Environment", "Serilog.Enrichers.Process", "Serilog.Enrichers.Thread" ],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "System": "Error",
        "Microsoft": "Error",
        "Otc": "Information",
        "Ole": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console",
              "Args": {
                "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
              }
            }
          ]
        }
      },
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "File",
              "Args": {
                "path": "logs/openpropostaapi-log-.txt",
                "rollingInterval": "Day"
              }
            }
          ]
        }
      }
    ],
    "Enrich": [
      "blablabla"
    ]
  },
  "AllowedHosts": "*",
  "RequestTrackerConfiguration": {
    "RequestTrackerEnabled": true
  },
  "JwtConfiguration": {
    "Audience": "ole open api",
    "Issuer": "ole consignado",
    "SecretKey": "abcdefghijklmnopqrstuvxz"
  },
  "DistributedCacheConfiguration": {
    "CacheStorageType": "Memory"
  }
}
